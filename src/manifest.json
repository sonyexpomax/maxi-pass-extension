{
  "manifest_version": 2,
  "name": "MaxiPass",
  "version": "0.1",
  "description": "MaxiPass Extension",
  "browser_action": {
      "default_icon": {
        "16": "images/lock.png",
        "32": "images/lock.png",
        "48": "images/lock.png",
        "128": "images/lock.png"
      },
      "default_title": "MaxiPass",
      "default_popup": "popup.html"
  },
  "icons": {
    "16": "images/lock.png",
    "32": "images/lock.png",
    "48": "images/lock.png",
    "128": "images/lock.png"
  },
  "author": "WizardsDev",
  "background": {
      "page": "background.html"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": [ "activeTab", "notifications" ],
  "minimum_chrome_version": "60"
}
